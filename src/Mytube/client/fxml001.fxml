<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Mytube.client.UIController">
  <left>
    <TreeView fx:id="treeview" editable="true" prefHeight="200.0" prefWidth="200.0" showRoot="false" />
  </left>
  <padding>
    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
  </padding>
   <top>
      <AnchorPane prefHeight="53.0" prefWidth="590.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutY="2.0" text="MYTUBE" textFill="#794a4a">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label layoutY="28.0" prefHeight="19.0" prefWidth="71.0" text="My labrary" />
            <Label layoutX="209.0" layoutY="27.0" prefHeight="21.0" prefWidth="74.0" text="My video" />
         </children>
      </AnchorPane>
   </top>
   <bottom>
      <AnchorPane prefHeight="46.0" prefWidth="590.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btn_store" layoutX="31.0" layoutY="12.0" mnemonicParsing="false" onAction="#store" prefHeight="23.0" prefWidth="56.0" text="저장" />
            <Button fx:id="btn_delete" layoutX="106.0" layoutY="12.0" mnemonicParsing="false" onAction="#delete_dialog" prefHeight="23.0" prefWidth="61.0" text="삭제" />
            <TextField fx:id="tf_search" layoutX="233.0" layoutY="1.0" prefHeight="25.0" prefWidth="237.0" />
            <Button fx:id="btn_search" layoutX="470.0" layoutY="2.0" mnemonicParsing="false" onAction="#search" prefHeight="25.0" prefWidth="65.0" text="검색" />
            <ImageView fx:id="image_showall" fitHeight="27.0" fitWidth="39.0" layoutX="541.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="btx_exit" blendMode="OVERLAY" layoutX="542.0" layoutY="3.0" mnemonicParsing="false" onAction="#imageShowall" prefHeight="26.0" prefWidth="37.0" text=" " />
         </children>
      </AnchorPane>
   </bottom>
   <right>
      <WebView fx:id="web" prefHeight="315.0" prefWidth="560.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
